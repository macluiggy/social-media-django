{
  "name": "nest-angular-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "bun run nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:prod:bun": "bun dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:no-watch": "vitest run --config ./vitest.config.ts",
    "test:all": "vitest run --config ./vitest.config.ts && vitest run --config ./vitest.config.e2e.ts",
    "test": "vitest --config ./vitest.config.ts --watch",
    "test:cov": "vitest --config ./vitest.config.ts --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:no-watch": "vitest run --config ./vitest.config.e2e.ts",
    "test:e2e": "vitest --config ./vitest.config.e2e.ts",
    "test:jest": "jest",
    "test:vitest": "vitest --config ./vitest.config.ts",
    "test:bun": "bun test --watch",
    "typeorm": "npx ts-node -r tsconfig-paths/register --transpile-only ./node_modules/typeorm/cli.js",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/db/config/typeorm.config.ts",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./src/db/migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./src/db/config/datasource.config.ts migration:revert",
    "db:create": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs db:create",
    "db:drop": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs db:drop",
    "seed:run": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run seed -d src/db/seeders/index.ts",
    "seed:create": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:create --name src/db/seeders/$npm_config_name",
    "seed": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d src/db/seeders/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.577.0",
    "@aws-sdk/s3-request-presigner": "^3.577.0",
    "@faker-js/faker": "^8.4.1",
    "@fastify/static": "^7.0.4",
    "@huggingface/inference": "^2.7.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/platform-fastify": "^10.3.8",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.5",
    "@types/bcrypt": "^5.0.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/uuid": "^9.0.8",
    "@vitest/coverage-v8": "^1.6.0",
    "aws-sdk": "^2.1622.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "install": "^0.13.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.5.1",
    "unplugin-swc": "^1.4.5",
    "vite": "^5.2.11",
    "vite-plugin-node": "^3.1.0",
    "vitest": "^1.6.0",
    "wrangler": "^3.56.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/jest": "^0.2.36",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
